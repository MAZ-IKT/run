FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      N5;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    WALL
    {
        type            slip;
    }
    MOVINGWALL
    {
        type            slip;
    }
    INLET
    {
	type		inletOutlet;
	inletValue	uniform 85;
    }
    OUTLET
    {
        type            zeroGradient;
    }
    FRONTANDBACK
    {
        type            empty;
    }
}


